                        : <a rel="tag" class="tag" href="/tag/cellmnc410/">cell:mnc=410</a>
                    
                        : <a rel="tag" class="tag" href="/tag/cellnetworkcingular/">cell:network=Cingular</a>
                    
                        : <a rel="tag" class="tag" href="/tag/celltagged/">celltagged</a>
                    
